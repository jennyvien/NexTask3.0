<AnchorPane prefHeight="400.0" prefWidth="270.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.Controller">
   <children>
      <Button fx:id="button" layoutX="109.0" layoutY="2.0" mnemonicParsing="false" onAction="#generateRandomTask" prefHeight="44.0" prefWidth="270.0" text="NexTask" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <font>
            <Font name="Segoe UI" size="36.0" />
         </font>
      </Button>
      <ListView fx:id="listView" editable="true" layoutY="67.0" onKeyPressed="#processKeyPressed" prefHeight="260.0" prefWidth="270.0" AnchorPane.bottomAnchor="73.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="67.0" />    
      <TextField fx:id="textField" alignment="TOP_LEFT" layoutY="327.0" onAction="#processText" prefHeight="74.0" prefWidth="270.0" promptText="Specify task and hit enter." AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" />
      <DialogPane fx:id="dialogPane" headerText="Your Next Task: " layoutX="17.0" layoutY="147.0" prefHeight="100.0" prefWidth="236.0" />
   </children>
</AnchorPane>
